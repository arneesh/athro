{
  "name": "athro",
  "author": "arneeshaima",
  "version": "1.5.4",
  "description": "A library for javascript which contains basic datastructures, algorithms and some generic functionalities which a developer needs",
  "main": "index.js",
  "scripts": {
    "dev-history": "ts-node src/index.ts",
    "dev-transpile": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "ts-node-dev --respawn src/index.ts",
    "test": "jest --forceExit --verbose --detectOpenHandles",
    "test-with-coverage": "jest --forceExit --coverage --verbose",
    "lint": "eslint --fix",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "serve": "node dist/index.js",
    "start-js": "yarn run serve",
    "deploy:dev": "",
    "deploy:prod": "",
    "doc": "jsdoc -c jsdoc.json"
  },
  "keywords": [
    "data structures",
    "algorithms",
    "generics",
    "stack",
    "queue",
    "linked list",
    "doubly linked list",
    "trees",
    "binary search tree",
    "bst",
    "tree traversal",
    "inorder",
    "preorder",
    "postorder",
    "breadth first search",
    "depth first search",
    "front-end",
    "backend",
    "date",
    "decimal",
    "titlecase",
    "searching",
    "sorting",
    "graphs",
    "trees",
    "reactjs",
    "vuejs",
    "javascript",
    "nodejs",
    "angular",
    "expressjs",
    "ember.js",
    "meteor",
    "mithril",
    "backbone.js",
    "polymer",
    "svelte"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "16.11.22",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "jsdoc": "^3.6.10",
    "lint-staged": "^12.3.3",
    "microbundle": "^0.15.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  }
}
